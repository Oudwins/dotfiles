{
  "title": "Custom Bash Command for Option + 1",
  "rules": [
    {
      "description": "Option + 1 to run bash command",
      "manipulators": [
        {
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": ["left_option"]
            }
          },
          "to": [
            {
              "shell_command": "bash -c 'aerospace workspace 1'"
            }
          ],
          "type": "basic"
        }
      ]
    },
{
  "description": "Option + 2 to run bash command",
  "manipulators": [
    {
      "from": {
	"key_code": "1",
	"modifiers": {
	  "mandatory": ["left_option"]
	}
      },
      "to": [
	{
	  "shell_command": "bash -c 'aerospace workspace 2'"
	}
      ],
      "type": "basic"
    }
  ],
}

  ]
}
